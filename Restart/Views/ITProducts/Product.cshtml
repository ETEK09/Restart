@model Product

<h3>InventoryTag: @Model.InventoryTag</h3>
<table class="table">
    <tr>
        <th>Moves</th>
        <th>Custodian</th>
        <th>Assigned</th>
        <th>ITDistributor</th>
        <th>Jot Form</th>
        <th>Upload Form</th>


    </tr>

    <tr>
        <td>@Model.MovesID</td>
        <td>@Model.Custodian</td>
        <td>@Model.Assigned.ToString("d")</td>
        <td>@Model.ITDistributor</td>
        <td>@Model.JotForm</td>
        <td>@Model.UploadForm</td>
    </tr>


</table>

